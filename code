#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int billamt,amtgiven;
int q,r;
scanf("%d",&amtgiven);
scanf("%d",&billamt);
q = amtgiven/billamt;
r = amtgiven%billamt;
printf("Quotient:%d\nRemainder:%d",q,r);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
